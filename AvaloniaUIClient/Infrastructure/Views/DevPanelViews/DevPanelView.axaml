<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="AvaloniaUIClient.Infrastructure.Views.DevPanelViews.DevPanelView"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Grid>
        <TabControl>
            <!--  Logs  -->
            <TabItem Header="Logs">
                <ContentControl Content="{Binding LogsPanelVmd}" />
            </TabItem>
        </TabControl>
    </Grid>

</UserControl>
