<UserControl
    x:Class="AvaloniaUIClient.Infrastructure.Views.DevPanelViews.DevView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <TabControl>

            <!--  Logs  -->
            <TabItem Header="Logs">
                <ContentControl Content="{Binding LogsPanelVmd}" />
            </TabItem>

            <!--  Stores  -->
            <TabItem Header="Stores">
                <ContentControl Content="{Binding StoresPanelVmd}" />
            </TabItem>
        </TabControl>
    </Grid>

</UserControl>
