<UserControl
    x:Class="AvaloniaUIClient.Infrastructure.Views.DevPanelViews.NotificationView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:devPanelVmds="clr-namespace:Core.VMD.DevPanelVmds;assembly=Client-Core"
    d:DesignWidth="800"
    d:DesignHeight="450"
    d:DataContext="{devPanelVmds:NotificationsVmd}"
    mc:Ignorable="d">

    <WrapPanel>
        <Button Content="Notify" Command="{Binding Notify}" />
    </WrapPanel>
</UserControl>
